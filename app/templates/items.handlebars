<h2>Available Robots</h2>
<div class='row'>
  <div class='col-lg-2'>
    <h4>All Robots</h4>
    <ul class='list-unstyled'>
      {{# each}}
        <li>
          {{# link-to "item" this }}
            <img {{ bind-attr src=avatar }} />
            {{ name }}
          {{/ link-to }}
        </li>
      {{/ each }}
    </ul>
  </div>
  <div class='col-lg-10'>
    {{outlet}}
  </div>
</div>
